msgid ""
"      defp ensure_loaded(module) do\n"
"        case Code.ensure_loaded(module) do\n"
"          { :module, _ } ->\n"
"            []\n"
"          { :error, _ } ->\n"
"            parent = Process.get(:elixir_parent_compiler)\n"
"            send parent, { :waiting, Process.self, module }\n"
"            receive do\n"
"              { :release, ^parent } -> ensure_loaded(module)\n"
"            end\n"
"        end\n"
"      end\n"
"    end"
