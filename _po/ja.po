"  defp do_interleave(a, b, acc) do\n"
"    case a.({ :cont, acc }) do\n"
"      { :suspended, acc, a } ->\n"
"        case b.({ :cont, acc }) do\n"
"          { :suspended, acc, b } ->\n"
"            do_interleave(a, b, acc)\n"
"          { :halted, acc } ->\n"
"            acc\n"
"          { :done, acc } ->\n"
"            finish_interleave(a, acc)\n"
"        end\n"
"      { :halted, acc } ->\n"
"        acc\n"
"      { :done, acc } ->\n"
"        finish_interleave(b, acc)\n"
